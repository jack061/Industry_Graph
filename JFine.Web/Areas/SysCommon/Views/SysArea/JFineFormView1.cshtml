<script>
    var keyValue = $.request("keyValue");
    $(function () {
	    initControl();
        if (!!keyValue) {
            $.ajax({
                url: "/SysCommon/SysArea/GetFormJson",
                data: { keyValue: keyValue },
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#form1").formSerialize(data);
                }
            });
        }
    });

	//初始化组件
    function initControl() {
        
    }

    function submitForm() {
        if (!$('#form1').formValid()) {
            return false;
        }
        var postData = $("#form1").formSerialize();
        $.submitForm({
            url: "/SysCommon/SysArea/SaveForm?keyValue=" + keyValue,
            param: postData,
            success: function () {
                $.currentWindow().$("#gridList").trigger("reloadGrid");
            }
        })
    }
</script>

