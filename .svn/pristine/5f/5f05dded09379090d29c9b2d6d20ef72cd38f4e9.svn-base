    <style>

        body {
            overflow: hidden;
            position: relative;
            width: 100%;
            height: 100%;
            margin: 0;
            padding: 0;
            /*background-color: #242152;*/
            background: -webkit-linear-gradient( top, #2e287a, #0277bd);
            /* Safari 5.1 - 6.0 */
            background: -o-linear-gradient( top, #2e287a, #0277bd);
            /* Opera 11.1 - 12.0 */
            background: -moz-linear-gradient( top, #2e287a, #0277bd);
            /* Firefox 3.6 - 15 */
            background: linear-gradient( top, #2e287a, #0277bd);
            -webkit-user-select: none;
            /*禁用手机浏览器的用户选择功能 */
            -moz-user-select: none;
            -webkit-touch-callout: none;
            /*系统默认菜单被禁用*/
            -webkit-user-select: none;
            /*webkit浏览器*/
            -khtml-user-select: none;
            /*早期浏览器*/
            -moz-user-select: none;
            /*火狐*/
            -ms-user-select: none;
            /*IE10*/
            user-select: none;
        }

        .blockDiv {
            float: left;
            width: 19.64%;
            margin-left: 0.3%;
            margin-top: 0.3%;
            height: 48%;
            background-color: #f5f5f5;
        }

        .titleDiv {
            position: relative;
            width: 100%;
            height: 10%;
            background-color: #7385eb;
            color: #FFFFFF;
            padding-top: 0px;
            text-align: center;
        }

        .titleSpan {
            font-size: 16px;
            text-align: center;
            position: relative;
            top: -4px;
        }

        /*.chartDiv {
            position: relative;
            width: 70%;
            height: 45%;
            background-color: blue;
            float: left;
        }*/

        .imgDiv {
            position: relative;
            width: 100%;
            height: 45%;
            float: left;
        }

        .bottomDiv {
            position: relative;
            width: 100%;
            height: 45%;
            background-color: green;
            float: left;
        }
        .progressBar {
            width: 80%;
            height: 18px;
            border: 1px solid #98AFB7;
            border-radius: 5px;
        }

        .bar {
            width: 0px;
            height: 18px;
            border-radius: 5px;
            background: #5EC4EA;
        }
    </style>
    <script>
        $(function () {
            progressBar();
        })
        function progressBar() {
            //初始化js进度条
            $(".bar").css("width", "0px");
            //进度条的速度，越小越快
            var speed = 20;
            bar = setInterval(function () {
                nowWidth = parseInt($(".bar").width());
                //宽度要不能大于进度条的总宽度
                if (nowWidth <= 200) {
                    barWidth = (nowWidth + 1) + "px";
                    $(".bar").css("width", barWidth);
                } else {
                    //进度条读满后，停止
                    clearInterval(bar);
                }
            }, speed);
        }

    </script>
</head>
