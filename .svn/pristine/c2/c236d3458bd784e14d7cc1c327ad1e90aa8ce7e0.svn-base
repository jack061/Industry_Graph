    function initPhotos(photo, photoUrl) {
        if (!photo) {
            return;
        }
        var photoArray = photo.split('|');
        var photoUrlArray = photoUrl.split('|');
        if (photoArray.length != photoUrlArray.length) {
            return;
        }
        for (var i = 0; i < photoArray.length; i++) {
