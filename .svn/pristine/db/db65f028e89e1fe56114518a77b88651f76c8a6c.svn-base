<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：参数配置 ================== -->
  <!-- 启用系统日志-->
  <add key="IsLog" value="true" />
  <add key="LogFilePath" value="D://JFine_log//SysLog" />
  <!-- 数据库超时间-->
  <add key="CommandTimeout" value="180" />
  <!--启用IP过滤 -->
  <add key="IsIPFilter" value="false" />
  <!--设置时段过滤 -->
  <add key="IsTimeFilter" value="false" />
  <!--系统初始密码-->
  <add key="InitPassword" value="123456"/>
  <!--数据库类型MySql、SqlServer-->
  <add key="DatabaseType" value="SqlServer"/>
  <!--数据库是否加密-->
  <add key="ConStringEncrypt" value="true" />
  
  <!--验证服务地址 -->
  <add key="CAHost" value="http://localhost:30844"/>
  <!--AI服务地址 -->
  <add key="AIHost" value="http://localhost:6060"/> 
  <!--AI_OCR服务地址 -->
  <add key="AI_OCRHost" value="http://localhost:6080"/>
  <!-- 软件名称 -->
  <add key="SoftName" value="JFine快速开发框架" />
  <!-- 软件版本 -->
  <add key="Version" value="3.0" />
  <!-- 联系我们 -->
  <add key="Contact" value="www.jfine.cn" />
  <!-- ================== 2：外部邮件参数 ================== -->
  <!-- 设置邮箱主机 -->
  <add key="MailHost" value="jfine.com" />
  <!-- 设置邮箱名称 -->
  <add key="MailName" value="JFine快速开发框架" />
  <!-- 设置邮箱地址 -->
  <add key="MailUser" value="jfine@163.com" />
  <!-- 设置邮箱密码 -->
  <add key="MailPassword" value="1234" />

  <!--邮件模板-->
  <add key="VailCode" value="您申请找回账号密码的验证码为：CODE,请注意保护您的账号密码！【JFine平台】" />
  
  <!-- ================== 3：Quartz参数 ================== -->
  <!-- 配置线程池 -->
  <add key="quartz.threadPool.type" value="Quartz.Simpl.SimpleThreadPool, Quartz"/>
  <add key="quartz.threadPool.threadCount" value="10"/>
  <add key="quartz.threadPool.threadPriority" value="Normal"/>
  <!-- 配置作业存储 -->
  <add key="quartz.jobStore.misfireThreshold" value="60000"/>
  <add key="quartz.jobStore.type" value="Quartz.Impl.AdoJobStore.JobStoreTX, Quartz"/>
  <add key="quartz.jobStore.useProperties" value="true"/>
  <add key="quartz.jobStore.dataSource" value="default"/>
  <add key="quartz.jobStore.tablePrefix" value="QRTZ_"/>
  <add key="quartz.jobStore.driverDelegateType" value="Quartz.Impl.AdoJobStore.StdAdoDelegate, Quartz"/>
  <add key="quartz.jobStore.lockHandler.type" value="Quartz.Impl.AdoJobStore.UpdateLockRowSemaphore, Quartz"/>
  <add key="quartz.dataSource.default.provider" value="SqlServer-20"/> 
  <!--<add key="quartz.dataSource.default.provider" value="MySql-65"/>-->

  <!-- ================== 4：供应商默认数据 ================== -->
  <add key="defaultOrgID" value="30a557b1-5a76-4dab-af63-75838ecd2439"/>
  <add key="defaultDeptID" value="975655493261463552"/>
  <add key="defaultRoleID" value="975655032546529280"/>
  
  <!-- ================== 5：Redis配置 ================== -->
  <add key="RedisConnectionString" value="127.0.0.1:6379"/>
  <add key="Redis.DefaultKey" value="jfine"/>

  <!-- ================== 6：MQTT配置 ================== -->
  <add key="MQTT_HOST" value="127.0.0.1"/>
  <add key="MQTT_PORT" value="1883"/>
  
</appSettings>